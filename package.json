{
  "name": "camping-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "rimraf .next && next dev --turbo",
    "dev:server": "cross-env NODE_ENV=development node server/index.js",
    "build": "next build",
    "start": "concurrently \"next start -p $PORT\" \"npm run start:server\"",
